<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>


<AnchorPane prefHeight="200.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.editDialog.EditController">
   <children>
      <GridPane layoutX="51.0" layoutY="55.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label text="Id" />
            <Label text="Name" GridPane.rowIndex="1" />
            <Label text="Age" GridPane.rowIndex="2" />
            <TextField fx:id="fieldName" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <TextField fx:id="fieldAge" GridPane.columnIndex="1" GridPane.rowIndex="2" />
            <TextField fx:id="fieldId" disable="true" editable="false" GridPane.columnIndex="1" />
         </children>
      </GridPane>
      <Label layoutX="113.0" layoutY="4.0" text="Edit person" />
      <Button layoutX="85.0" layoutY="155.0" mnemonicParsing="false" onAction="#saveButtonPressed" prefHeight="31.0" prefWidth="131.0" text="Save" />
   </children>
</AnchorPane>
